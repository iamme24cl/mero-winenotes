<div class="container mt-5">
  <div class="row mb-5">
    <div class="col text-center fs-3">
      <h2>Members</h2>
    </div>
  </div>

  <div class="row justify-content-center">
    <div class="col-md-6 text-center">
      <ul class="list-group">
        <% @users.each do |user| %>
          <li class="list-group-item"><a href="/users/<%= user.id %>"><%= user.name.titleize %></a></li>
        <% end %>
      </ul>
    </div>
  </div>

</div>